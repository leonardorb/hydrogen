    "axios": "^0.9.1",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.3",
    "lodash": "^4.10.0",
    "nedb": "^1.8.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
