{
  "name": "hydrogen",
  "version": "0.0.1",
  "description": "An opinionated package for awesomeness.",
  "main": "http://localhost:8080",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js ./test/mocha-style-no-op.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "nwjs",
    "webpack",
    "react",
    "react-router",
    "redux",
    "nedb",
    "bootstrap",
    "font-awesome"
  ],
  "author": "Leonardo R. Barbosa",
  "license": "MIT",
  "single_instance": true,
  "window": {
    "title": "Hydrogen",
    "icon": "",
    "toolbar": true,
    "frame": true,
    "width": 1024,
    "height": 800,
    "min_width": 1024,
    "min_height": 800,
    "resizable": true
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "font-awesome-loader": "0.0.1",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.0.1",
    "resolve-url-loader": "^1.4.3",
